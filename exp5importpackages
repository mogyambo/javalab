package A1;

public class Addition {
	int a=10;
	int b=20;
	
	public int add()
	{
	  return a+b;
	}
}

package A2;

public class Substraction {
	int a=40;
	int b=30;
	public int subtract()
	{
		return a-b;
	}
}

package A3;
import A1.Addition;
import A2.Substraction;
public class Main {
	public static void main(String[] args) {
		Addition obj1=new Addition();
		Substraction obj2=new Substraction();		
		System.out.println("The addition is "+obj1.add());
		System.out.println("The subtraction is "+obj2.subtract());
	}
	
}
